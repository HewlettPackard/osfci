export TLS_CERT_PATH=
export TLS_KEY_PATH=
# Previous one are in the case of using a self signed certificate
export DNS_DOMAIN=
# Previous one is in the case of using a fully qualified domain name with letsencrypt
export CERT_STORAGE=
